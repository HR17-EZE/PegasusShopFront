{
  "name": "pegasusshopfront",
  "version": "1.0.0",
  "description": "npm for FEC",
  "main": "index.js",
  "scripts": {
    "start": "nodemon Server/index.js",
    "dev:react": "webpack --config ./webpack.config.js --mode development -w",
    "db:setup": "node database-mongodb/seed.js",
    "test": "jest",
    "dev:server": "npx nodemon ./Server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexTing1/PegasusShopFront.git"
  },
  "author": "Alex, Alex, and Elizabeth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlexTing1/PegasusShopFront/issues"
  },
  "homepage": "https://github.com/AlexTing1/PegasusShopFront#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "babel-jest": "^27.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^27.0.4",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/react": "^11.2.7",
    "babel-loader": "^8.2.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "keywords": [],
  "jest": {
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared",
      "Server"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
